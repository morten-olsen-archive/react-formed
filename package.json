{
  "name": "react-formed",
  "version": "1.0.7",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test:mocha": "mocha --require @babel/register --require jsdom-global/register --require ./test/setup test/**/*.spec.jsx",
    "test:nyc": "nyc npm run test:mocha",
    "test:lint": "eslint src test",
    "test": "npm run test:lint && npm run test:nyc",
    "build": "babel -d lib src",
    "bundle": "webpack",
    "npm audit": "lint-staged",
    "prepublish": "npm run build && npm run bundle",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "release": "git add --all && git cz && git push"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "require": [
      "@babel/register",
      "jsdom-global/register",
      "./test/setup"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "insertPragma": true,
    "arrowParens": "always"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^7.0.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "semantic-release": "^15.13.16",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-export-namespace-from"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://git.mortenolsen.pro/tools/react/react-formed"
  },
  "dependencies": {
    "lint-staged": "^8.2.1"
  }
}
