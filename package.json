{
  "name": "react-formed",
  "version": "2.0.0-alpha.1",
  "description": "",
  "main": "lib/index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/morten-olsen/react-formed"
  },
  "scripts": {
    "test:jest": "jest",
    "test:lint": "eslint src/**/*.ts test/**/*.tsx",
    "test": "npm run test:jest && npm run test:lint",
    "build": "babel -d lib --extensions .ts,.tsx src",
    "bundle": "webpack",
    "npm audit": "lint-staged",
    "prepublish": "npm run build && npm run bundle",
    "coverage": "coveralls"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "require": [
      "@babel/register",
      "jsdom-global/register",
      "./test/setup"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.10.1",
    "@types/mocha": "^5.2.7",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.21",
    "@types/react-redux": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^8.2.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-export-namespace-from"
    ]
  },
  "dependencies": {
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0"
  }
}
